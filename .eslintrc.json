{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jest": true
  },
    "extends": "google",
  "parserOptions": {
        "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "no-console": 0,
    "no-alert": 0,
    "no-underscore-dangle": 0,
    "no-restricted-globals": 0,
        "linebreak-style": 0,
        "require-jsdoc": 0,
        "new-cap": 0,
        "max-len": 0
    }
}
